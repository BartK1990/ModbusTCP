<Window x:Class="ModbusTCP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModbusTCP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListView x:Name="Logger" HorizontalAlignment="Left" Height="200" Margin="10,209,0,0" VerticalAlignment="Top" Width="772" IsSynchronizedWithCurrentItem="False">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="SetIP" Content="SET IP" HorizontalAlignment="Left" Height="37" Margin="47,98,0,0" VerticalAlignment="Top" Width="82"/>
        <TextBox x:Name="IPAddress" HorizontalAlignment="Left" Height="37" Margin="191,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262"/>

    </Grid>
</Window>
